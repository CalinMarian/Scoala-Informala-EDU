<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net5.0-windows</TargetFramework>
    <RootNamespace>Scoala_Informala_EDU</RootNamespace>
    <UseWindowsForms>true</UseWindowsForms>
    <ApplicationIcon>csharp.ico</ApplicationIcon>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Resources\GetStarted.JPG" />
    <None Remove="Resources\LearnPicture-Intro.JPG" />
    <None Remove="Resources\LearnPicture-Methods.JPG" />
    <None Remove="Resources\LearnPicture-Methods.png" />
    <None Remove="Resources\LearnPicture-PrimateDataType.png" />
    <None Remove="Resources\LearnPicture-PrimitiveDataType.bmp" />
    <None Remove="Resources\Methodssss.JPG" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="Properties\Resources.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>Resources.resx</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="Properties\Resources.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>Resources.Designer.cs</LastGenOutput>
    </EmbeddedResource>
  </ItemGroup>

</Project>